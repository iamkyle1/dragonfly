<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skt.realedu.mission.dragonfly"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="7" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="com.skt.realedu.mission.dragonfly.GameTitle"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.skt.realedu.mission.dragonfly.GameHelp"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.skt.realedu.mission.dragonfly.GamePlay"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.skt.realedu.mission.dragonfly.GameResult"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.skt.realedu.mission.dragonfly.Collection"
            android:screenOrientation="landscape" />
    </application>

</manifest>